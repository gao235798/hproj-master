    <select id="selectByALL" parameterType="java.lang.String" resultMap="ResultMapWithBLOBs">
        select
        <include refid="Base_Column_List"/>
        ,
        b.cInvCName,c.cComUnitName
        from xzys_ha_Inventory a LEFT JOIN InventoryClass b ON a.cInvCCode=b.cInvCCode
        left join ComputationUnit c on a.cComUnitCode=c.cComUnitCode
        where a.tstatus='0' and ( a.cInvCCode like '1%' or a.cInvCCode like '2%' or a.cInvCCode like '502%' or
        a.cInvCCode like '507%' or a.cInvCCode like '509%' or a.cInvCCode like '6%' or a.cInvCCode like '7%') order by
        a.ttype
    </select>


